# Copy this file to .env for local testing
# DO NOT commit .env file with real values!

# User Information
FIRST_NAME=Test
LAST_NAME=User
NUMBER_OF_TICKETS=1
EMAIL=test@example.com
DOB_MONTH=1
DOB_DAY=1
DOB_YEAR=1990
ZIP=10001
COUNTRY=USA

# Testing Configuration
# Set to true to fill forms but not submit (for testing)
DRY_RUN=true

# Advanced Configuration (optional)
# Number of retry attempts for failed operations (default: 2)
MAX_RETRIES=2
# Page operation timeout in milliseconds (default: 30000)
PAGE_TIMEOUT=30000
# Navigation timeout in milliseconds (default: 60000)
NAVIGATION_TIMEOUT=60000
# Random delay range in milliseconds (default: 1000-3000)
MIN_DELAY=1000
MAX_DELAY=3000